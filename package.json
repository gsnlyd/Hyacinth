{
  "name": "hyacinth",
  "description": "hyacinth electron app",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/js/backend/main.js",
  "scripts": {
    "start": "electron .",
    "buildcss": "npx tailwindcss -i ./css/tailwind.css -o ./dist/css/style.css",
    "watchcss": "npx tailwindcss -i ./css/tailwind.css -o ./dist/css/style.css --watch",
    "buildjsfront": "rollup -c",
    "buildjsback": "tsc --project tsconfig.backend.json",
    "wf": "rollup -c -w",
    "wb": "tsc --project tsconfig.backend.json --watch",
    "elrebuild": "electron-rebuild"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "electron": "^13.1.5",
    "electron-rebuild": "^2.3.5",
    "rollup": "^2.52.7",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "better-sqlite3": "^7.4.1",
    "nifti-reader-js": "^0.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
