{
  "name": "hyacinth",
  "description": "hyacinth electron app",
  "version": "1.0.0",
  "private": true,
  "main": "./built/js/backend/main.js",
  "scripts": {
    "start": "electron .",
    "buildcss": "npx tailwindcss -i ./css/tailwind.css -o ./built/css/style.css",
    "watchcss": "npx tailwindcss -i ./css/tailwind.css -o ./built/css/style.css --watch",
    "buildjsfront": "rollup -c",
    "buildjsback": "tsc --project tsconfig.backend.json",
    "wf": "rollup -c -w",
    "wb": "tsc --project tsconfig.backend.json --watch",
    "elrebuild": "electron-rebuild"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/react": "^17.0.14",
    "@types/react-router": "^5.1.16",
    "electron": "^13.6.1",
    "electron-packager": "^15.4.0",
    "electron-rebuild": "^3.2.5",
    "rollup": "^2.52.7",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.2",
    "@tensorflow/tfjs": "^3.11.0",
    "better-sqlite3": "^7.4.4",
    "cornerstone-core": "^2.6.0",
    "cornerstone-wado-image-loader": "^4.0.4",
    "daikon": "^1.2.42",
    "dicom-parser": "^1.8.11",
    "nifti-reader-js": "^0.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "timsort": "^0.3.0"
  }
}
