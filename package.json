{
  "name": "hyacinth",
  "description": "hyacinth electron app",
  "version": "1.0.0",
  "private": true,
  "main": "./built/js/backend/main.js",
  "scripts": {
    "start": "electron .",
    "buildstart": "node build.js && ELECTRON_ENABLE_LOGGING=true HYACINTH_DEV=true electron .",
    "buildcss": "npx tailwindcss -i ./css/tailwind.css -o ./built/css/style.css",
    "test": "npm run buildjstest && ELECTRON_RUN_AS_NODE=true NODE_ENV=test electron built/js/testbundle.js",
    "buildjstest": "esbuild src/test/test.ts --outfile=built/js/testbundle.js --external:better-sqlite3 --bundle --platform=node --format=cjs --target=node14",
    "elrebuild": "electron-rebuild"
  },
  "devDependencies": {
    "@types/react": "^17.0.14",
    "electron": "^13.6.1",
    "electron-packager": "^15.4.0",
    "electron-rebuild": "^3.2.5",
    "esbuild": "^0.14.2",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.2",
    "@tensorflow/tfjs": "^3.11.0",
    "better-sqlite3": "^7.4.4",
    "daikon": "^1.2.42",
    "nifti-reader-js": "^0.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "timsort": "^0.3.0"
  }
}
